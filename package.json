{
  "name": "cypress-starter-kit",
  "version": "1.0.0",
  "description": "A starter kit for Cypress E2E and accessibility testing with TypeScript and cypress-axe.",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "cypress run",
    "test:ally": "cypress run --spec 'tests/accessibility/**/*.spec.ts'",
    "test:cloud": "dotenv -- cypress run --record",
    "test:ally:cloud": "dotenv -- cypress run --spec 'tests/accessibility/**/*.spec.ts' --record",
    "lint": "eslint . --ext .ts,.js",
    "type-check": "tsc --noEmit",
    "clean:screenshots": "rm -rf cypress/screenshots/*"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "cypress": "^15.0.0",
    "cypress-axe": "^1.6.0",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "cypress",
    "e2e",
    "accessibility",
    "axe-core",
    "typescript",
    "testing"
  ]
}
